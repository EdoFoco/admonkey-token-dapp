{
  "name": "www",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "dotenv": "^7.0.0",
    "moment": "^2.24.0",
    "openzeppelin-solidity": "^3.1.0",
    "truffle": "^5.3.10",
    "truffle-hdwallet-provider": "^1.0.5"
  },
  "scripts": {
    "compile": "truffle compile --all",
    "test": "truffle test --compile-all",
    "test:debug": "node inspect ./node_modules/truffle/build/cli.bundled.js test --compile-all",
    "deploy:dev": "truffle migrate --reset --compile-all",
    "deploy:mainnet": "truffle migrate --reset --compile-all --network mainnet",
    "deploy:rinkeby": "truffle migrate --reset --compile-all --network rinkeby"
  }
}